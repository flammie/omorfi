# {{{ options
STRICT-TAGS +=
    # Universal POS <http://universaldependencies.org/fi/pos/index.html>
    UPOS=NOUN
    UPOS=VERB
    UPOS=AUX
    UPOS=ADJ
    UPOS=ADV
    UPOS=ADP
    UPOS=PROPN
    UPOS=PRON
    UPOS=SYM
    UPOS=PUNCT
    UPOS=CCONJ
    UPOS=SCONJ
    UPOS=INTJ
    UPOS=NUM
    UPOS=DET
    UPOS=X
    # Universal features <http://universaldependencies.org/fi/feat/index.html>
    Case=Abe
    Case=Abl
    Case=Acc
    Case=Ade
    Case=All
    Case=Ela
    Case=Ess
    Case=Gen
    Case=Ill
    Case=Ine
    Case=Ins
    Case=Lat
    Case=Nom
    Case=Par
    Case=Tra
    Case=Com
    Clitic=Han
    Clitic=Ka
    Clitic=Kaan
    Clitic=Kin
    Clitic=Ko
    Clitic=Pa
    Clitic=S
    Connegative=Yes
    Degree=Comp
    Degree=Pos
    Degree=Sup
    Derivation=Ja
    Derivation=Inen
    Derivation=Va
    Derivation=La
    Derivation=Minen
    Derivation=Nen
    Derivation=Sti
    Derivation=U
    InfForm=1
    InfForm=2
    InfForm=3
    Mood=Cnd
    Mood=Imp
    Mood=Ind
    Mood=Pot
    Number=Plur
    Number[psor]=Plur
    Number[psor]=Sing
    Number=Sing
    NumType=Card
    NumType=Ord
    PartForm=1
    PartForm=2
    PartForm=3
    PartForm=Agt
    PartForm=Neg
    PartForm=Past
    PartForm=Pres
    Person=0
    Person=1
    Person=2
    Person=3
    Person[psor]=1
    Person[psor]=2
    Person[psor]=3
    Polarity=Neg
    PronType=Int
    PronType=Dem
    PronType=Prs
    PronType=Rel
    Style=Arch
    Style=Coll
    Tense=Past
    Tense=Pres
    Typo=Yes
    Voice=Act
    Voice=Pss
    # Omorfi specific features
    <InfForm=4>
    <Lexicalised=Sti>
    <Position=Final>
    <Position=Initial>
    <*GUESSER=NONE>
    <*lowercased>
    <*UPPERCASED>
    <*Titlecased>
    <*dETITLECASED>
    <Blacklisted=FGK>
    <Blacklisted=PROPN-BLOCKING>
    <GoesWith=Missing->
    <AffixType=Prefix>
    <PropnType=First>
    <PropnType=Last>
    ;
OPTIONS += no-inline-sets ;

DELIMITERS = "<.>" "<!>" "<?>" ;

# }}}

SETS

# {{{ grammar groups
## Nominal poses
LIST NOMINAL = UPOS=NOUN UPOS=ADJ UPOS=PRON UPOS=PROPN ;
## Clitic particle tags
LIST CLITIC = Clitic=Han Clitic=Ko Clitic=Pa Clitic=S Clitic=Ka Clitic=Kaan Clitic=Kin ;
## Conjunctions
LIST CONJUNCTION = UPOS=CCONJ UPOS=SCONJ ;
## any vebs
LIST VERBAUX = UPOS=VERB UPOS=AUX ;
## copula?
LIST COPULA = ("olla" UPOS=AUX) ;
##
LIST VERB-PL3 = (UPOS=VERB Number=Plur Person=3) (UPOS=AUX Number=Plur Person=3) ;
## ad-adjectives etc.
LIST ADA = UPOS=ADV ;
## AD words
LIST AD = UPOS=ADJ UPOS=ADV UPOS=ADP ;
## comparatives and superlatives
LIST CMP-SUP = Degree=Comp Degree=Sup ;
##
LIST POSS = Number[psor]=Sing Number[psor]=Plur Person[psor]=1 Person[psor]=2 Person[psor]=3 ;
## agent participle
LIST AGENT-PCP = PartForm=Agt ;
## case subgroups
LIST LOCATIVE-IN = Case=Ine Case=Ade ;
LIST LOCATIVE-TO = Case=Ill Case=All ;
LIST LOCATIVE-FROM = Case=Ela Case=Abl ;
LIST ILL/ALL/PAR = Case=Ill Case=All Case=Par ;
LIST PERS-PRON-SG = (PronType=Prs Number=Sing) ;
# ordinals XXX: adj doesn't have ord yet...
LIST NUM-ORD = (UPOS=NUM NumType=Ord) ;
LIST ADJ-ORD = (UPOS=ADJ NumType=Ord) ;
# Infintive a
LIST INFA-SG = (InfForm=1 Number=Sing) ;
LIST INFA-PL = (InfForm=1 Number=Plur) ;
LIST INFA = (InfForm=1) ;
# Infinitive ma + ill
LIST INFMAAN = (InfForm=1 Number=Sing Case=Ill) ;
# adj cases (and other agreement stuffs)
LIST ADJ-PAR = (UPOS=ADJ Case=Par) (UPOS=DET Case=Par) (UPOS=PRON PronType=Dem Case=Par) (UPOS=NUM Case=Par) ;
LIST ADJ-INE = (UPOS=ADJ Case=Ine) (UPOS=DET Case=Ine) (UPOS=PRON PronType=Dem Case=Ine) (UPOS=NUM Case=Ine) ;
LIST ADJ-ELA = (UPOS=ADJ Case=Ela) (UPOS=DET Case=Ela) (UPOS=PRON PronType=Dem Case=Ela) (UPOS=NUM Case=Ela) ;
LIST ADJ-ILL = (UPOS=ADJ Case=Ill) (UPOS=DET Case=Ill) (UPOS=PRON PronType=Dem Case=Ill) (UPOS=NUM Case=Ill) ;
LIST ADJ-ADE = (UPOS=ADJ Case=Ade) (UPOS=DET Case=Ade) (UPOS=PRON PronType=Dem Case=Ade) (UPOS=NUM Case=Ade) ;
LIST ADJ-ABL = (UPOS=ADJ Case=Abl) (UPOS=DET Case=Abl) (UPOS=PRON PronType=Dem Case=Abl) (UPOS=NUM Case=Abl) ;
LIST ADJ-ALL = (UPOS=ADJ Case=All) (UPOS=DET Case=All) (UPOS=PRON PronType=Dem Case=All) (UPOS=NUM Case=All) ;
LIST ADJ-TRA = (UPOS=ADJ Case=Tra) (UPOS=DET Case=Tra) (UPOS=PRON PronType=Dem Case=Tra) (UPOS=NUM Case=Tra) ;
LIST ADJ-ABE = (UPOS=ADJ Case=Abe) (UPOS=DET Case=Abe) (UPOS=PRON PronType=Dem Case=Abe) (UPOS=NUM Case=Abe) ;
LIST ADJ-ESS = (UPOS=ADJ Case=Ess) (UPOS=DET Case=Ess) (UPOS=PRON PronType=Dem Case=Ess) (UPOS=NUM Case=Ess) ;
# inmpv sg2 is systematically indv pres sg3
LIST IMPV-SG2 = (Mood=Imp Person=2 Number=Sing) ;
LIST INDV-SG3 = (Mood=Ind Person=3 Number=Sing) ;
# Negation
LIST NEG-AUX = (UPOS=AUX Polarity=Neg) ;
# Personals
LIST PRS-1SG = (UPOS=PRON Person=1 Number=Sing) ;
LIST PRS-2SG = (UPOS=PRON Person=2 Number=Sing) ;
LIST PRS-3SG = (UPOS=PRON Person=3 Number=Sing) ;
LIST PRS-1PL = (UPOS=PRON Person=1 Number=Plur) ;
LIST PRS-2PL = (UPOS=PRON Person=2 Number=Plur) ;
LIST PRS-3PL = (UPOS=PRON Person=3 Number=Plur) ;
LIST PRS-1SG-SUBJECT = (UPOS=PRON Person=1 Number=Sing Case=Nom) ;
LIST PRS-2SG-SUBJECT = (UPOS=PRON Person=2 Number=Sing Case=Nom) ;
LIST PRS-3SG-SUBJECT = (UPOS=PRON Person=3 Number=Sing Case=Nom) ;
LIST PRS-1PL-SUBJECT = (UPOS=PRON Person=1 Number=Plur Case=Nom) ;
LIST PRS-2PL-SUBJECT = (UPOS=PRON Person=2 Number=Plur Case=Nom) ;
LIST PRS-3PL-SUBJECT = (UPOS=PRON Person=3 Number=Plur Case=Nom) ;
# }}}
# {{{ sentences and clauses
LIST BEGINSENT = >>> ;
LIST ENDSENT = "<.>" "<!>" "<?>" ;
LIST CLB = "<,>" ;
LIST QMARK = "<?>" ;
# }}}
# {{{ word groups

## short words that many find problems with. This is a strong filter, for more
## careful filters use other groups like RARER-NOUN
