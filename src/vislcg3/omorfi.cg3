SELECT:probably MIN-WEIGHT ;
REMOVE:avoid-instructives Case=Ins (NOT 0 UPOS=VERB + InfForm=2) ;
REMOVE:avoid-abessives Case=Abe (NOT 0 UPOS=VERB + InfForm=3) ;
REMOVE:avoid-comitatives Case=Com ;
REMOVE:avoid-clitics SUB:* CLITIC ;
REMOVE:blacklists SUB:* BLACKLISTED ;
REMOVE:avoid-sg0 Person=0 ;
REMOVE:rare-pers-pron-forms Number=Plur (0 PERS-PRON-SG) ;
REMOVE:adj-over-drv-va Derivation=Va (0 UPOS=ADJ) ;
REMOVE:prefer-pri-sg3-over-imp-sg2 IMPV-SG2 (0 INDV-SG3) ;
REMOVE:prefer-original-case RECASED (NOT -1 BEGINSENT);

REMOVE:odd-adjs WHAT-THE-ADJ ;



# not all lines sent to disambiguator are sentences with a verb but
# if a user inputs verbless sentence they can expect such parses
SELECT:try-to-find-one-verb-per-sentence UPOS=VERB (NOT 0*C UPOS=VERB) ;

SELECT:adverb-last-resort UPOS=ADV (-1C UPOS=VERB) ;


SELECT:numnom-last-resort Case=Nom (0 NUMNOMGENS) ;

